<body class="text-center">

  <main class="form-signin w-100 m-auto">
    <form action="/details" method="POST" class="signin">


      <h1 class="h3 mb-3 fw-normal">Details</h1>


      <div class="form-floating">
        <input name="total" type="text" class="form-control top" id="floatingInput1" placeholder="Total classes" required autofocus >
        <label for="floatingInput">Total classes happened till now</label>
      </div>

      <div class="form-floating">
        <input name="current" type="text" class="form-control bottom" id="floatingInput2" placeholder="Attended classes"    required autofocus>
        <label for="floatingPassword">Total classes you have attended till now</label>
      </div>

      <div class="form-floating">
        <input name="semester" type="text" class="form-control bottom" id="floatingInput2" placeholder="Semester" minlength="1" pattern="\d*" maxlength="1" required autofocus>
        <label for="floatingPassword">Semester</label>
      </div>


      <div class="form-floating">
        <input name="minper" type="text" class="form-control bottom" id="floatingInput3" placeholder="Semester Last Date" minlength="1" pattern="\d*" maxlength="2" required autofocus>
        <label for="floatingPassword">Minimum Percentage required</label>
      </div>



      <button class="w-100 btn btn-lg btn-primary" type="submit">Proceed</button>

      <p class="mt-5 mb-3 text-muted">&copy; 2023</p>
    </form>
  </main>


